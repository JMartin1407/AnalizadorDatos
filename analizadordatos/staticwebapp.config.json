{
  "routes": [
    {
      "route": "/_next/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/_next/*", "/static/*", "/*.{css,js,json,png,jpg,svg,ico,woff,woff2}"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "networking": {
    "allowedIpRanges": []
  }
}
