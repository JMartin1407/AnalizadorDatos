{
  "routes": [
    {
      "route": "/api/*",
      "serve": "/api/index.js",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.css", "/*.js"]
  },
  "networking": {
    "allowedIpRanges": []
  }
}
